{
  "name": "play-djmax",
  "displayName": "Play DJMAX",
  "description": "Play the music from DJMAX archive",
  "version": "1.0.1",
  "publisher": "minwook-shin",
  "author": {
    "name": "minwook-shin"
  },
  "homepage": "https://github.com/minwook-shin/vscode-play-djmax",
  "bugs": {
    "url": "https://github.com/minwook-shin/vscode-play-djmax/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/minwook-shin/vscode-play-djmax.git"
  },
  "keywords": [
    "djmax",
    "youtube"
  ],
  "engines": {
    "vscode": "^1.31.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:djmax.playlist"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "djmax.playlist",
        "title": "DJMAX: Show YouTube Playlist"
      }
    ],
    "keybindings": [
      {
        "command": "djmax.playlist",
        "key": "ctrl+d ctrl+j",
        "mac": "cmd+d cmd+j"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "14.x",
    "@types/vscode": "^1.31.0",
    "@vscode/test-electron": "^2.1.3",
    "eslint": "^8.14.0",
    "glob": "^8.0.1",
    "mocha": "^9.2.2",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "node-cache": "^5.1.2",
    "ytpl": "^2.3.0"
  }
}